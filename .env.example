# Copy to .env.local for local development.
# All client-side env vars must be prefixed with REACT_APP_ (Create React App).

# Backend API base URL (no trailing slash)
REACT_APP_API_BASE_URL=http://127.0.0.1:8000/api

# Paystack public key (safe to expose in frontend; use test key for dev)
# Test: pk_test_...
# Live: pk_live_...
REACT_APP_PAYSTACK_PUBLIC_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
